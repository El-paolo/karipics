<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>User:</strong>
    <%= post.user_id %>
  </p>

  <div class="col-6">
  
  <%= image_tag post.images , class:"img-fluid w-25 float-left" %>
  </div>

</div>
